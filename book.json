{
  "title": "Cloudzp's book",
  "author": "Cloudzp",
  "description": "学习笔记",
  "language": "zh-hans",
  "gitbook": "3.2.3",
  "styles": {
    "website": "./styles/website.css"
  },
  "structure": {
    "readme": "whoami.md"
  },
  "links": {
    "sidebar": {
      "GitHub": "https://github.com/Cloudzp"
    }
  },
  "plugins": [
    "-sharing",
    "splitter",
    "expandable-chapters-small",
    "anchors",
    "theme-comscore",
    "github",
    "github-buttons",
    "donate",
    "sharing-plus",
    "anchor-navigation-ex",
    "favicon",
    "autotheme"
  ],
  "pluginsConfig": {
    "theme-default": {
       "showLevel": true,
       "theme": "dark"
    },
    "autotheme": {
      "white": [9, 10, 11, 12, 13, 14, 15, 16],
      "sepia": [6, 7, 8, 17, 18, 19],
      "night": [20, 21, 22, 23, 0, 1, 2, 3, 4, 5]
    },
    "github": {
      "url": "https://github.com/Cloudzp"
    },
    "github-buttons": {
      "buttons": [{
        "user": "Cloudzp",
        "repo": "booklet",
        "type": "star",
        "size": "small",
        "count": true
      }
      ]
    },
    "donate": {
      "alipay": "/source/images/donate.jpg",
      "title": "",
      "button": "赞赏",
      "alipayText": " "
    },
    "sharing": {
      "douban": true,
      "facebook": true,
      "google": true,
      "hatenaBookmark": false,
      "instapaper": false,
      "line": false,
      "linkedin": false,
      "messenger": false,
      "pocket": false,
      "qq": false,
      "qzone": false,
      "stumbleupon": false,
      "twitter": false,
      "viber": false,
      "vk": false,
      "weibo": true,
      "whatsapp": false,
      "all": [
        "google", "facebook", "weibo", "twitter",
        "qq", "qzone", "linkedin", "pocket"
      ]
    },
    "anchor-navigation-ex": {
      "showLevel": false
    },
    "favicon":{
      "shortcut": "./source/images/favicon.jpg",
      "bookmark": "./source/images/favicon.jpg",
      "appleTouch": "./source/images/apple-touch-icon.jpg",
      "appleTouchMore": {
        "120x120": "./source/images/apple-touch-icon.jpg",
        "180x180": "./source/images/apple-touch-icon.jpg"
      }
    }
  }
}